---
import type { InferGetStaticParamsType, GetStaticPaths } from "astro";
import SectionLayout from "@layouts/section.astro";
import config from "@page-configs/example-structure.ts";
import staticPaths from "@data/example-structure/static-paths.json";

type Params = InferGetStaticParamsType<typeof getStaticPaths>;

const { slug } = Astro.params as Params;

export const getStaticPaths = (async () => {
  return staticPaths;
}) satisfies GetStaticPaths;
---

<SectionLayout config={config} slug={slug} />
