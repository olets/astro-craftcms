---
import CraftCMSFetch from '@lib/craft-cms/fetch';
import url from '@lib/craft-cms/url';
import { type Data, cacheDirectory, query } from '@config/routes/example-route';

const data = await CraftCMSFetch<Data>({ cacheDirectory, query });
---

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>sectionHandle</th>
      <th>sectionId</th>
      <th>title</th>
      <th>typeHandle</th>
      <th>typeId</th>
      <th>url</th>
      <th>visit</th>
    </tr>
  </thead>
  <tbody>
    {
      data?.entries.map((entry) => (
        <tr>
          <td>{entry.id}</td>
          <td>{entry.sectionHandle}</td>
          <td>{entry.sectionId}</td>
          <td>{entry.title}</td>
          <td>{entry.typeHandle}</td>
          <td>{entry.typeId}</td>
          <td>{url(entry.uri)}</td>
          <td>
            <a href={url(entry.uri)}>Go</a>
          </td>
        </tr>
      ))
    }
  </tbody>
</table>
