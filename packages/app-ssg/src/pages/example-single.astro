---
import getEntry from '@lib/craft-cms/get-entry';
import type { Entry } from '@page-configs/example-single';
import { query, uriPrefix } from '@page-configs/example-single';

const entry = await getEntry<Entry>({ query, uriPrefix });

if (entry === undefined) {
  return new Response(null, { status: 404 });
}
---

{entry.title}
